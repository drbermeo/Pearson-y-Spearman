/*El coeficiente de correlación de Pearson*/
//1.Calcular la medias aritmeticas de la columna user_followers_count y de la columna user_friends_count
val media_X: List[Tuits] => Double =
(x: List[Tuits]) => x.map(_.user_followers_count) //Obtengo lista de la variable x( user_followers_count)
  .sum //suma de los user_followers_count
  .toDouble /* Transformo la suma de entero a double*/ / tuits.length //obtengo el tamaño de los tuits
val media_Y: List[Tuits] => Double =
  (y: List[Tuits]) => y.map(_.user_friends_count) //obtengo lista de la variable y( user_friends_count)
    .sum //suma de los user_friends_count
    .toDouble /* Transformo la suma de entero a double*/ / tuits.length //obtengo el tamaño de los tuits

val mx: Double = media_X(tuits) // Guarda el reultado de la funcion media_X
val my: Double = media_Y(tuits) // Guarda el resultado de la funcion media_Y

def correlacionPearson (r:List[Tuits], mx:Double,my:Double): Double = {
  //1. Calcular la covarianza de (x,y)
  val covarianza: Double = tuits.map(t =>
    (t.user_followers_count - mx) //Obtengo cada elemento de la lista x( user_followers_count) y le resto su mx
      *
      (t.user_friends_count - my)) //Obtengo cada elemento de la lista y( user_friends_count) y le resto su my
    .sum /*suma del producto de estas dos variables  */ / tuits.length //obtengo el tamaño de los tuits
  //2. Calculo la varianza en X ( user_followers_count) y en Y ( user_friends_count)
  val varianzax: Double = tuits.map(
     // Esto eleva al cuadrado la diferencia entre el valor de user_followers_count de cada Tuits y la media mx.
    t => (math.pow(t.user_followers_count - mx, 2)))
    .sum /*suma de la lista resultante *// tuits.length //obtengo el tamaño de los tuits

  val varianzay: Double = tuits.map(
    t => (math.pow(t.user_friends_count - my, 2)))
    .sum/*suma de la lista resultante */  / tuits.length //obtengo el tamaño de los tuits

   // FORMULA GENERAL: Calcula la covarianza dividida por la raíz cuadrada del producto de las varianzas.
  covarianza / (math.sqrt(varianzax * varianzay))
}

correlacionPearson(tuits,mx,my) // LLamo a la funcion.


// Coeficientes de correlación de Spearman.

//La función rangos asigna rangos a cada valor basándose en cuántos valores son estrictamente menores que cada elemento en la lista.
def rangos(data: List[Int]): List[Double] =
  data.map(d =>
      data.count(_ < d)//Cuenta cuantos elementos en data son menores a d
        + 1) // Suma al resultado para que empiecen en 1
    .map(_.toDouble) // Convierte cada rango calculado a double

def coeficienteSpearman(tuits: List[Tuits]): Double = {
  // Uso la funcion rangos para extrar los rangos de las variables user_followers_count y user_friends_count de la  lista de tuits
  val x = rangos(
    tuits.map(_.user_followers_count)) // Contiene solo los valores de user_followers_count
  val y = rangos(
    tuits.map(_.user_friends_count)) // Contiene solo los valores de user_friends_count

  // Diferencias al cuadrado entre los rangos de user_followers_count y user_friends_count para cada elemento en tu conjunto de datos.
  val d = x.zip(y). // Combina las dos listas en una lista de tuplas.
    map { case /*desestructurar cada tupla en sus componentes individuales.*/
      (rx, ry)  // Par de rangos correspondientes en las listas x - y.
       =>
      math.pow(rx - ry, 2) // calcula la diferencia entre los rangos y la eleva al cuadrado
    }
  val sumatoriaD = d.sum //Sumatoria de todos los elementos de d
  //Formula general:
  1 - (6 * sumatoriaD) /
    (tuits.length /* Se obtiene el tamañaño de los tuits*/ * (math.pow(tuits.length, 2) - 1))
}

coeficienteSpearman(tuits) // asigna el resultado de la llamada de la funcion
